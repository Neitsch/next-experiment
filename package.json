{
  "name": "next-js",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon server/launch.ts",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "NODE_ENV=production node .next/production-server/server/launch.js",
    "test": "jest -c jest.config.js --silent --ci -i",
    "lint": "jest -c jest.lint.config.js --silent --ci",
    "format": "prettier --loglevel silent --write '{,!(node_modules)/**/}*.{ts,tsx,js,css,md}'",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "heroku-postbuild": "yarn run build",
    "gen:schema:graphql": "ts-node --compilerOptions '{\"module\":\"commonjs\"}' typeDefs/printSchema.ts",
    "gen:schema:json": "apollo-codegen introspect-schema schema.graphql --output schema.json",
    "gen:schema:types": "mkdir query-types && apollo-codegen generate '{,!(node_modules)/**/}*.{ts,tsx,js}' --target typescript --output query-types --schema ./schema.json",
    "gen:schema": "yarn run gen:schema:graphql && yarn run gen:schema:json && yarn run gen:schema:types",
    "clear-jest": "jest --clearCache"
  },
  "dependencies": {
    "@material-ui/core": "3.0.2",
    "@material-ui/icons": "3.0.1",
    "@types/axe-webdriverjs": "2.0.0",
    "@types/body-parser": "1.17.0",
    "@types/compression": "0.0.36",
    "@types/cookie-parser": "1.4.1",
    "@types/cors": "2.8.4",
    "@types/express": "4.16.0",
    "@types/express-graphql": "0.6.2",
    "@types/express-jwt": "0.0.40",
    "@types/express-rate-limit": "^2.9.3",
    "@types/helmet": "0.0.42",
    "@types/jest": "23.3.9",
    "@types/joi": "^14.0.0",
    "@types/js-cookie": "2.2.0",
    "@types/jwt-decode": "2.2.1",
    "@types/next": "7.0.5",
    "@types/node": "^10.12.10",
    "@types/react": "16.7.7",
    "@types/selenium-webdriver": "3.0.13",
    "@types/uuid": "3.4.4",
    "@zeit/next-typescript": "1.1.1",
    "apollo-cache-hermes": "0.8.6",
    "apollo-cache-inmemory": "1.3.11",
    "apollo-cache-persist": "0.1.1",
    "apollo-client": "2.4.7",
    "apollo-errors": "1.9.0",
    "apollo-link-context": "1.0.10",
    "apollo-link-http": "1.5.7",
    "apollo-link-persisted-queries": "0.2.2",
    "apollo-server-express": "2.2.3",
    "auth0-lock": "11.12.1",
    "compression": "1.7.3",
    "cookie-parser": "1.4.3",
    "cors": "2.8.5",
    "enzyme": "3.7.0",
    "enzyme-to-json": "3.3.4",
    "express": "4.16.4",
    "express-graphql": "0.7.1",
    "express-jwt": "5.3.1",
    "express-jwt-authz": "1.0.0",
    "express-rate-limit": "^3.3.2",
    "graphql": "14.0.2",
    "graphql-tag": "2.10.0",
    "graphql-validation-complexity": "0.2.4",
    "helmet": "3.15.0",
    "husky": "1.2.0",
    "isomorphic-unfetch": "3.0.0",
    "joi": "^14.3.0",
    "js-cookie": "2.2.0",
    "jss": "9.8.7",
    "jwks-rsa": "1.3.0",
    "jwt-decode": "2.2.0",
    "localforage": "1.7.3",
    "next": "7.0.0",
    "next-routes": "1.4.2",
    "next-with-apollo": "3.3.0",
    "parse-database-url": "0.3.0",
    "pg": "^7.6.1",
    "react": "16.6.3",
    "react-apollo": "2.3.1",
    "react-dom": "16.6.3",
    "react-jss": "8.6.1",
    "reflect-metadata": "^0.1.12",
    "sqlite3": "^4.0.4",
    "typeorm": "^0.2.9",
    "typescript": "3.1.6",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.1.6",
    "axe-webdriverjs": "2.1.0",
    "babel-core": "6.26.3",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-graphql-tag": "1.6.0",
    "enzyme-adapter-react-16": "1.7.0",
    "jest": "23.6.0",
    "jest-environment-jsdom": "^23.4.0",
    "jest-environment-jsdom-global": "1.1.0",
    "jest-extended": "^0.11.0",
    "jest-runner-prettier": "0.2.6",
    "jest-runner-tslint": "1.0.5",
    "nodemon": "1.18.6",
    "prettier": "1.15.2",
    "react-styleguidist": "8.0.6",
    "selenium-webdriver": "4.0.0-alpha.1",
    "ts-jest": "23.1.4",
    "ts-node": "7.0.1",
    "tslint-config-prettier": "1.16.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn test"
    }
  }
}
